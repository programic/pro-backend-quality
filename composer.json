{
  "name": "programic/pro-backend-quality",
  "description": "Programic Quality Control package",
  "keywords": [
    "quality",
    "codestyle",
    "phpmd",
    "ecs",
    "phpstan",
    "phpmnd"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Programic",
      "email": "info@programic.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "larastan/larastan": "^3.0",
    "phpmd/phpmd": "^2.12",
    "slevomat/coding-standard": "^8.3",
    "symplify/coding-standard": "^12.0",
    "symplify/easy-coding-standard": "^12.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2 || ^0.7",
    "phpstan/phpdoc-parser": "^2.0",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-nette": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpstan/phpstan-webmozart-assert": "^2.0",
    "squizlabs/php_codesniffer": "^3.7.1"
  },
  "require-dev": {
    "orchestra/testbench": "^7.1 | ^8.0 | ^9.0"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Programic\\ProQuality\\": "src/",
      "ProgramicCodingStandards\\": "ecs/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Programic\\ProQuality\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Programic\\ProQuality\\ProQualityServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
