{
  "name": "programic/pro-backend-quality",
  "description": "Programic Quality Control package",
  "keywords": [
    "quality",
    "codestyle",
    "phpmd",
    "ecs",
    "phpstan",
    "phpmnd",
    "phpcpd"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Programic",
      "email": "info@programic.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "nunomaduro/larastan": "^1.0 || ^2.0",
    "phpmd/phpmd": "^2.12",
    "povils/phpmnd": "^2.4",
    "sebastian/phpcpd": "^6.0",
    "slevomat/coding-standard": "^7.0",
    "symplify/coding-standard": "^9.3",
    "symplify/easy-coding-standard": "^9.4",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2 || ^0.7",
    "phpstan/phpdoc-parser": "^1.0.0",
    "phpstan/phpstan": "^1.0",
    "squizlabs/php_codesniffer": "^3.6.2"
  },
  "require-dev": {
    "orchestra/testbench": "^7.1"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Programic\\ProQuality\\": "src/",
      "ProgramicCodingStandards\\": "ecs/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Programic\\ProQuality\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Programic\\ProQuality\\ProQualityServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
